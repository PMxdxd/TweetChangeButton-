{

    "manifest_version": 2,
    "name": "TweetChangeButton",
    "version": "1.0",
   
    "description": "ツイートを好きな文字で変更できるボタンを追加します。",
    
    "icons": {
      "48": "icons/tw.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.twitter.com/*"],
        "js": ["tw.js"]
      }
    ]
  
  }